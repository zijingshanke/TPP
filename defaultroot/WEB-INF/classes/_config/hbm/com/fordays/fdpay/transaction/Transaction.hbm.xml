<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fordays.fdpay.transaction.Transaction"
	table="transaction"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_transaction</param>
          </generator>
    </id>

    <property
        name="amount"
        column="amount"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="action"
        column="action"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="type"
        column="type"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="accountDate"
        column="account_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="payDate"
        column="pay_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="refundsDate"
        column="refunds_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="refundsStatus"
        column="refunds_status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
	    <property
        name="closeDate"
        column="close_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
	    <property
        name="closeReson"
        column="close_reson"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="mark"
        column="mark"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
	    <property
        name="flagStatus"
        column="flag_status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="no"
        column="no"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>


    <many-to-one
        name="fromAgent"
        class="com.fordays.fdpay.agent.Agent"
        not-null="false"
    >

        <column name="from_agent_id" />
    </many-to-one>
        <many-to-one
        name="toAgent"
        class="com.fordays.fdpay.agent.Agent"
        not-null="false"
    >

        <column name="to_agent_id" />
    </many-to-one>

    <many-to-one
        name="orderDetails"
        class="com.fordays.fdpay.transaction.OrderDetails"
        not-null="false"
    >

        <column name="order_id" />
    </many-to-one>
</class>

</hibernate-mapping>