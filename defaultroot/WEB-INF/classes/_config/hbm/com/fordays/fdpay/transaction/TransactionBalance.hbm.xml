<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fordays.fdpay.transaction.TransactionBalance"
	table="TRANSACTION_BALANCE"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_transactionbalance</param>
          </generator>
    </id>

    <property
        name="transactionId"
        column="transaction_id"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="balance"
        column="balance"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="creditBalance"
        column="credit_balance"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="agentId"
        column="agent_id"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="transactionDate"
        column="transaction_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="notallowBalance"
        column="notallow_balance"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>



</class>

</hibernate-mapping>