<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fordays.fdpay.system.City"
	table="city"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_city</param>
          </generator>
    </id>

    <property
        name="cname"
        column="cname"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="ename"
        column="ename"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>



    <many-to-one
        name="province"
        class="com.fordays.fdpay.system.Province"
        not-null="false"
    >

        <column name="province_id" />
    </many-to-one>
  	<set
      name="banks"
      inverse="true"
  	  cascade="all-delete-orphan"
      >
          <key>
  		        <column name="city_id" />
          </key>
          <one-to-many 
              class="com.fordays.fdpay.system.Bank"
          />
    </set>
  	<set
      name="draws"
      inverse="true"
  	  cascade="all-delete-orphan"
      >
          <key>
  		        <column name="city_id" />
          </key>
          <one-to-many 
              class="com.fordays.fdpay.transaction.Draw"
          />
    </set>
  	<set
      name="accounts"
      inverse="true"
  	  cascade="all-delete-orphan"
      >
          <key>
  		        <column name="city_id" />
          </key>
          <one-to-many 
              class="com.fordays.fdpay.agent.Account"
          />
    </set>
</class>

</hibernate-mapping>